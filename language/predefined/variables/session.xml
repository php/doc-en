<refentry role="variable" xml:id="reserved.variables.session" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>$_SESSION</refname>
  <refpurpose>Session variables</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <para>
   An associative array containing session variables available to
   the current script. See the <link linkend="ref.session">Session
   functions</link> documentation for more information on how this
   is used.
  </para>
 </refsect1>

 <refsect1 role="notes">
  &reftitle.notes;
  &note.is-superglobal;
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>session_start</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
 
