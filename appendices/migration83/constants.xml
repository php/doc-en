 <sect2 xml:id="migration83.constants.zip">
  <title>Zip</title>

  <itemizedlist>
   <listitem>
    <simpara>
     <constant>ZipArchive::ER_DATA_LENGTH</constant> (libzip >= 1.10)
    </simpara>
   </listitem>
   <listitem>
    <simpara>
     <constant>ZipArchive::ER_NOT_ALLOWED</constant> (libzip >= 1.10)
    </simpara>
   </listitem>
