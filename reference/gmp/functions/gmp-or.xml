<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.gmp-or">
 <refnamediv>
  <refname>gmp_or</refname>
  <refpurpose>Bitwise OR</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>resource</type><methodname>gmp_or</methodname>
   <methodparam><type>resource</type><parameter>a</parameter></methodparam>
   <methodparam><type>resource</type><parameter>b</parameter></methodparam>
  </methodsynopsis>
  <para>
   Calculates bitwise inclusive OR of two GMP numbers.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>a</parameter></term>
     <listitem>
      &gmp.parameter; 
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>b</parameter></term>
     <listitem>
      &gmp.parameter; 
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &gmp.return;
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>gmp_or</function> example</title>
    <programlisting role="php">
<![CDATA[
<?php
$or1 = gmp_or("0xfffffff2", "4");
echo gmp_strval($or1, 16) . "\n";
$or2 = gmp_or("0xfffffff2", "2");
echo gmp_strval($or2, 16) . "\n";
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
fffffff6
fffffff2
]]>
    </screen>
   </example>
  </para>
 </refsect1>

</refentry>

