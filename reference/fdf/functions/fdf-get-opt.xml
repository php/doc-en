<refentry xml:id="function.fdf-get-opt" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>fdf_get_opt</refname>
  <refpurpose>Gets a value from the opt array of a field</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>mixed</type><methodname>fdf_get_opt</methodname>
   <methodparam><type>resource</type><parameter>fdf_document</parameter></methodparam>
   <methodparam><type>string</type><parameter>fieldname</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>element</parameter><initializer>-1</initializer></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
 </refsect1>

</refentry>

