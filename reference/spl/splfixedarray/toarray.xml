<refentry xml:id="splfixedarray.toarray" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SplFixedArray::toArray</refname>
  <refpurpose>Returns a PHP array from the fixed array</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="SplFixedArray">
   <modifier>public</modifier> <type>array</type><methodname>SplFixedArray::toArray</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Returns a PHP array from the fixed array.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns a PHP <type>array</type>, similar to the fixed array.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>SplFixedArray::toArray</function> example</title>
    <programlisting role="php">
<![CDATA[
<?php
$fa = new SplFixedArray(3);
$fa[0] = 0;
$fa[2] = 2;
var_dump($fa->toArray());
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
array(3) {
  [0]=>
  int(0)
  [1]=>
  NULL
  [2]=>
  int(2)
}
]]>
    </screen>
   </example>
  </para>
 </refsect1>

</refentry>
