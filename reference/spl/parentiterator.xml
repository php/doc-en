 <partintro>
 
<!-- {{{ ParentIterator intro -->
  <section xml:id="parentiterator.intro">
   &reftitle.intro;
   <para>
    This extended <classname>FilterIterator</classname> allows a recursive
    iteration using <classname>RecursiveIteratorIterator</classname> that only
    shows those elements which have children.
   </para>
  </section>
<!-- }}} -->
 
  <section xml:id="parentiterator.synopsis">
   &reftitle.classsynopsis;
 
<!-- {{{ Synopsis -->
   <classsynopsis>
    <ooclass><classname>ParentIterator</classname></ooclass>
 
<!-- {{{ Class synopsis -->
    <classsynopsisinfo>
     <ooclass>
      <classname>ParentIterator</classname>
     </ooclass>

     <ooclass>
      <modifier>extends</modifier>
      <classname>RecursiveFilterIterator</classname>
     </ooclass>

     <oointerface>
      <interfacename>RecursiveIterator</interfacename>
     </oointerface>

     <oointerface>
      <interfacename>OuterIterator</interfacename>
     </oointerface>
 
    </classsynopsisinfo>
<!-- }}} -->
 
