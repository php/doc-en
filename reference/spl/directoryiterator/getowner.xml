<refentry xml:id="directoryiterator.getowner" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DirectoryIterator::getOwner</refname>
  <refpurpose>Get owner of current DirectoryIterator item</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="DirectoryIterator">
   <modifier>public</modifier> <type>int</type><methodname>DirectoryIterator::getOwner</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Get the owner of the current 
   <classname>DirectoryIterator</classname> item,
   in numerical format.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   The file owner of the file, in numerical format.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><methodname>DirectoryIterator::getOwner</methodname> example</title>
    <para>
     This example displays the owner of the directory which contains the script.
    </para>
    <programlisting role="php">
<![CDATA[
<?php
$iterator = new DirectoryIterator(dirname(__FILE__));
print_r(posix_getpwuid($iterator->getOwner()));
?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
Array
(
    [name] => tom
    [passwd] => x
    [uid] => 501
    [gid] => 42
    [gecos] => Tom Cat
    [dir] => /home/tom
    [shell] => /bin/bash
)
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>DirectoryIterator::getGroup</methodname></member>
    <member><methodname>DirectoryIterator::getiNode</methodname></member>
    <member><methodname>DirectoryIterator::getPerms</methodname></member>
    <member><function>posix_getpwuid</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

