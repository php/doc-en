<refentry xml:id="splheap.valid" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SplHeap::valid</refname>
  <refpurpose>Check whether the heap contains more nodes</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>SplHeap::valid</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Checks if the heap contains any more nodes.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns &true; if the heap contains any more nodes, &false; otherwise.
  </para>
 </refsect1>

</refentry>

