<refentry xml:id="limititerator.seek" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>LimitIterator::seek</refname>
  <refpurpose>Seek to the given position</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>LimitIterator::seek</methodname>
   <methodparam><type>int</type><parameter>position</parameter></methodparam>
  </methodsynopsis>
  <para>
   Moves the iterator to the offset specified by <parameter>position</parameter>.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>position</parameter></term>
     <listitem>
      <para>
       The position to seek to.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the offset position after seeking.
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   Throws an <classname>OutOfBoundsException</classname> if the position is 
   outside of the limits specified in <methodname>LimitIterator::__construct</methodname>.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>LimitIterator::current</methodname></member>
    <member><methodname>LimitIterator::key</methodname></member>
    <member><methodname>LimitIterator::rewind</methodname></member>
    <member><methodname>LimitIterator::next</methodname></member>
    <member><methodname>LimitIterator::valid</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

