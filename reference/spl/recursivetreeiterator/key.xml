<refentry xml:id="recursivetreeiterator.key" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>RecursiveTreeIterator::key</refname>
  <refpurpose>Get the key of the current element</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>mixed</type><methodname>RecursiveTreeIterator::key</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Gets the current key prefixed and postfixed.
  </para>

  &warn.undocumented.func;

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the current key prefixed and postfixed.
  </para>
 </refsect1>


</refentry>
