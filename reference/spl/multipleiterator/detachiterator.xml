<refentry xml:id="multipleiterator.detachiterator" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>MultipleIterator::detachIterator</refname>
  <refpurpose>Detaches an iterator</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="MultipleIterator">
   <modifier>public</modifier> <type>void</type><methodname>MultipleIterator::detachIterator</methodname>
   <methodparam><type>Iterator</type><parameter>iterator</parameter></methodparam>
  </methodsynopsis>
  <para>
   Detaches an iterator.
  </para>

  &warn.undocumented.func;

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>iterator</parameter></term>
     <listitem>
      <para>
       The iterator to detach.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>MultipleIterator::attachIterator</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
