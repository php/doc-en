  <refentry id="function.pdf-setlinecap">
   <refnamediv>
    <refname>pdf_setlinecap</refname>
    <refpurpose>Sets linecap parameter</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>void</type><methodname>pdf_setlinecap</methodname>
      <methodparam><type>resource</type><parameter>pdfdoc</parameter></methodparam>
      <methodparam><type>int</type><parameter>linecap</parameter></methodparam>
     </methodsynopsis>
    <para>
      Set the <parameter>linecap</parameter> parameter to a value between 0 and 2 inclusive.
    </para>
   </refsect1>
  </refentry>

