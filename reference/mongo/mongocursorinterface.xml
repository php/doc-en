     <ooclass>
      <classname>MongoCursorInterface</classname>
     </ooclass>

     <ooclass>
      <modifier>extends</modifier>
      <classname>Iterator</classname>
     </ooclass>
    </classsynopsisinfo>
    <!-- }}} -->

    <classsynopsisinfo role="comment">&Methods;</classsynopsisinfo>
