<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="stomp.hasframe">
 <refnamediv>
  <refname>Stomp::hasFrame</refname>
  <refname>stomp_has_frame</refname>
  <refpurpose>Indicates whether or not there is a frame ready to read</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <simpara>&style.oop; (method):</simpara>
   <methodsynopsis>
   <modifier>public</modifier> <type>bool</type><methodname>Stomp::hasFrame</methodname>
   <void/>
   </methodsynopsis>
  <simpara>&style.procedural;:</simpara>
  <methodsynopsis>
   <type>bool</type><methodname>stomp_has_frame</methodname>
   <methodparam><type>resource</type><parameter>link</parameter></methodparam>
  </methodsynopsis>
  <simpara>
   Indicates whether or not there is a frame ready to read.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   &stomp.param.link;
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   Returns &true; if a frame is ready to read, or &false; otherwise.
  </simpara>
 </refsect1>

</refentry>
