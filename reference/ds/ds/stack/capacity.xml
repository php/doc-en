 <refnamediv>
  <refname>Ds\Stack::capacity</refname>
  <refpurpose>Returns the current capacity</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>Ds\Stack::capacity</methodname>
   <void />
  </methodsynopsis>
  <para>
    Returns the current capacity.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
    The current capacity.
  </para>
 </refsect1>
