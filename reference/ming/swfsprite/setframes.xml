<refentry xml:id="swfsprite.setframes" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SWFSprite->setFrames</refname>
  <refpurpose>Sets the total number of frames in the animation</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <classsynopsis>
   <ooclass><classname>SWFSprite</classname></ooclass>
   <methodsynopsis>
    <type>void</type><methodname>setFrames</methodname>
    <methodparam><type>int</type><parameter>number</parameter></methodparam>
   </methodsynopsis>
  </classsynopsis>
  &warn.experimental.func;
  <para>
   <function>swfsprite->setframes</function> sets the total number of frames 
   in the animation to <parameter>numberofframes</parameter>.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

</refentry>

