    <xi:include xpointer="xmlns(db=http://docbook.org/ns/docbook) xpointer(id('class.swffill')/db:refentry/db:refsect1[@role='description']/descendant::db:methodsynopsis[1])" />
 
   </classsynopsis>
<!-- }}} -->
 
  </section>
 </partintro>
 &reference.ming.entities.swffill;
 
