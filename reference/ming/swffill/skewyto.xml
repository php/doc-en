<refentry xml:id="swffill.skewyto" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SWFFill::skewYTo</refname>
  <refpurpose>Sets fill y-skew</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>SWFFill::skewYTo</methodname>
   <methodparam><type>float</type><parameter>y</parameter></methodparam>
  </methodsynopsis>
  &warn.experimental.func;
  <para>
   Sets the fill y-skew to <parameter>y</parameter>.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>y</parameter></term>
     <listitem>
      <para>
       When <parameter>y</parameter> is 1.0, it is a 45-degree upward slant.
       More is more upward, less is more downward.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>SWFFill::skewXTo</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

