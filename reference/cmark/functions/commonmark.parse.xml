<refentry xml:id="function.commonmark-parse" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>CommonMark\Parse</refname>
  <refpurpose>Parsing</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>CommonMark\Node</type><methodname>CommonMark\Parse</methodname>
   <methodparam><type>string</type><parameter>content</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>options</parameter></methodparam>
  </methodsynopsis>
  <para>
  Shall parse <parameter>content</parameter>
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>content</parameter></term>
    <listitem>
     <para>
      markdown string
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>options</parameter></term>
    <listitem>
     <para>
      A mask of:
     </para>
     <variablelist>
     <varlistentry>
      <term>
       <constant>CommonMark\Parser\Normal</constant>
       (<type>int</type>)
      </term>
      <listitem>
       <simpara>
       </simpara>
      </listitem>
     </varlistentry>
     <varlistentry>
      <term>
       <constant>CommonMark\Parser\Normalize</constant>
       (<type>int</type>)
      </term>
      <listitem>
       <simpara>
       </simpara>
      </listitem>
     </varlistentry>
     <varlistentry>
      <term>
       <constant>CommonMark\Parser\ValidateUTF8</constant>
       (<type>int</type>)
      </term>
      <listitem>
       <simpara>
       </simpara>
      </listitem>
     </varlistentry>
     <varlistentry>
      <term>
       <constant>CommonMark\Parser\Smart</constant>
       (<type>int</type>)
      </term>
      <listitem>
       <simpara>
       </simpara>
      </listitem>
     </varlistentry>
     </variablelist>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Shall return root CommonMark\Node
  </para>
 </refsect1>


</refentry>

