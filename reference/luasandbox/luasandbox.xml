<phpdoc:classref xml:id="class.luasandbox" xmlns:phpdoc="http://php.net/ns/phpdoc" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xi="http://www.w3.org/2001/XInclude">

 <title>The LuaSandbox class</title>
 <titleabbrev>LuaSandbox</titleabbrev>

 <partintro>

<!-- {{{ LuaSandbox intro -->
  <section xml:id="luasandbox.intro">
   &reftitle.intro;
   <para>
    The LuaSandbox class creates a Lua environment and allows for execution of
    Lua code.
   </para>
  </section>
<!-- }}} -->

  <section xml:id="luasandbox.synopsis">
   &reftitle.classsynopsis;

<!-- {{{ Synopsis -->
   <classsynopsis>
    <ooclass><classname>LuaSandbox</classname></ooclass>

<!-- {{{ Class synopsis -->
    <classsynopsisinfo>
     <ooclass>
      <classname>LuaSandbox</classname>
     </ooclass>
    </classsynopsisinfo>
<!-- }}} -->
    <classsynopsisinfo role="comment">Constants</classsynopsisinfo>
    <fieldsynopsis>
     <modifier>const</modifier>
     <type>int</type>
     <varname linkend="luasandbox.constants.samples">LuaSandbox::SAMPLES</varname>
     <initializer>0</initializer>
    </fieldsynopsis>
    <fieldsynopsis>
     <modifier>const</modifier>
     <type>int</type>
     <varname linkend="luasandbox.constants.seconds">LuaSandbox::SECONDS</varname>
     <initializer>1</initializer>
    </fieldsynopsis>
    <fieldsynopsis>
     <modifier>const</modifier>
     <type>int</type>
     <varname linkend="luasandbox.constants.percent">LuaSandbox::PERCENT</varname>
     <initializer>2</initializer>
    </fieldsynopsis>

    <classsynopsisinfo role="comment">&Methods;</classsynopsisinfo>
    <xi:include xpointer="xmlns(db=http://docbook.org/ns/docbook) xpointer(id('class.luasandbox')/db:refentry/db:refsect1[@role='description']/descendant::db:methodsynopsis[not(@role='procedural')])" />
   </classsynopsis>
<!-- }}} -->

  </section>

<!-- {{{ LuaSandbox constants -->
  <section xml:id="luasandbox.constants">
   &reftitle.constants;
   <variablelist>

    <varlistentry xml:id="luasandbox.constants.samples">
     <term><constant>LuaSandbox::SAMPLES</constant></term>
     <listitem>
      <para>
       Used with <methodname>LuaSandbox::getProfilerFunctionReport</methodname> to return timings in samples.
      </para>
     </listitem>
    </varlistentry>

    <varlistentry xml:id="luasandbox.constants.seconds">
     <term><constant>LuaSandbox::SECONDS</constant></term>
     <listitem>
      <para>
       Used with <methodname>LuaSandbox::getProfilerFunctionReport</methodname> to return timings in seconds.
      </para>
     </listitem>
    </varlistentry>

    <varlistentry xml:id="luasandbox.constants.percent">
     <term><constant>LuaSandbox::PERCENT</constant></term>
     <listitem>
      <para>
       Used with <methodname>LuaSandbox::getProfilerFunctionReport</methodname> to return timings in percentages of the total.
      </para>
     </listitem>
    </varlistentry>

   </variablelist>
  </section>
<!-- }}} -->


 </partintro>

 &reference.luasandbox.entities.luasandbox;

</phpdoc:classref>

