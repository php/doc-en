<refentry xml:id="reflectionfunctionabstract.getextensionname" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionFunctionAbstract::getExtensionName</refname>
  <refpurpose>Gets extension name</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type class="union"><type>string</type><type>false</type></type><methodname>ReflectionFunctionAbstract::getExtensionName</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Get the extensions name.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   The name of the extension which defined the function,
   or &false; for user-defined functions.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>ReflectionFunctionAbstract::getExtension</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
