<refentry xml:id="reflectionfunctionabstract.getparameters" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionFunctionAbstract::getParameters</refname>
  <refpurpose>Gets parameters</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>array</type><methodname>ReflectionFunctionAbstract::getParameters</methodname>
   <void />
  </methodsynopsis>
  <para>
   Get the parameters as an array of <type>ReflectionParameter</type>,
   in the order in which they are defined in the source.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   The parameters, as a <classname>ReflectionParameter</classname> object.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>ReflectionFunctionAbstract::getNumberOfParameters</methodname></member>
    <member><function>func_get_args</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

