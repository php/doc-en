   <classsynopsis>
<!-- {{{ hier -->
    <ooclass><modifier>abstract</modifier><classname>ReflectionFunctionAbstract</classname></ooclass>
    <oointerface><interfacename>Reflector</interfacename></oointerface>
<!-- }}} -->
