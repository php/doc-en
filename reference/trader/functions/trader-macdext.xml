<?xml version="1.0" encoding="utf-8"?>

<refentry xml:id="function.trader-macdext" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>trader_macdext</refname>
  <refpurpose>MACD with controllable MA type</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>trader_macdext</methodname>
   <methodparam><type>array</type><parameter>real</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>fastPeriod</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>fastMAType</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>slowPeriod</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>slowMAType</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>signalPeriod</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>signalMAType</parameter></methodparam>
  </methodsynopsis>
  <para>

  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>real</parameter></term>
    <listitem>
     <para>
      &trader.arg.array.of.real;
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>fastPeriod</parameter></term>
    <listitem>
     <para>
      &trader.arg.fast.period;
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>fastMAType</parameter></term>
    <listitem>
     <para>
      &trader.arg.fast.ma.type;
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>slowPeriod</parameter></term>
    <listitem>
     <para>
      &trader.arg.slow.period;
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>slowMAType</parameter></term>
    <listitem>
     <para>
      &trader.arg.slow.ma.type;
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>signalPeriod</parameter></term>
    <listitem>
     <para>
      &trader.arg.signal.period;
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>signalMAType</parameter></term>
    <listitem>
     <para>
      &trader.arg.signal.ma.type;
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>Returns an array with calculated data or false on failure.</para>
 </refsect1>


</refentry>

