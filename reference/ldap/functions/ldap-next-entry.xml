     &ldap.changelog.ldap-object;
     &ldap.changelog.entry-object;
     &ldap.changelog.return-result-object;
    </tbody>
   </tgroup>
  </informaltable>
 </refsect1>
 
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>ldap_get_entries</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
