<refentry xml:id="function.ldap-exop-whoami" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ldap_exop_whoami</refname>
  <refpurpose>WHOAMI extended operation helper</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type class="union"><type>string</type><type>false</type></type><methodname>ldap_exop_whoami</methodname>
   <methodparam><type>LDAP\Connection</type><parameter>ldap</parameter></methodparam>
  </methodsynopsis>
  <para>
   Performs a WHOAMI extended operation and returns the data.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>ldap</parameter></term>
    <listitem>
     <para>
      &ldap.parameter.ldap;
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   The data returned by the server, or &false; on error.
  </para>
 </refsect1>

 <refsect1 role="changelog">
  &reftitle.changelog;
