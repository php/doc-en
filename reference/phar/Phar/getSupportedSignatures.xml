<refentry xml:id="phar.getsupportedsignatures" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Phar::getSupportedSignatures</refname>
  <refpurpose>Return array of supported signature types</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>Phar::getSupportedSignatures</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Return array of supported signature types
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   No parameters.
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns an array containing any of <literal>MD5</literal>, <literal>SHA-1</literal>,
   <literal>SHA-256</literal>, <literal>SHA-512</literal>, or <literal>OpenSSL</literal>.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>Phar::getSignature</function></member>
    <member><function>Phar::setSignatureAlgorithm</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

