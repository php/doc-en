<refentry xml:id="messagepack.unpack" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>MessagePack::unpack</refname>
  <refpurpose>Unserialize data</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="MessagePack">
   <modifier>public</modifier> <type>mixed</type><methodname>MessagePack::unpack</methodname>
   <methodparam><type>string</type><parameter>serialized</parameter></methodparam>
   <methodparam choice="opt">
    <type class="union"><type>array</type><type>string</type><type>object</type></type>
    <parameter>template</parameter>
    <initializer>&null;</initializer>
   </methodparam>
  </methodsynopsis>
  <para>
   Unserializes data with MessagePack.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>serialized</parameter></term>
     <listitem>
      <para>
       The serialized data to unserialize.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>template</parameter></term>
     <listitem>
      <para>
       A template &array;, class name or &object; to fill.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the unserialized data or &null; if a template was provided but it could not be filled with the unserialized data.
  </para>
 </refsect1>

</refentry>
