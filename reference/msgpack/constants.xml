<appendix xml:id="msgpack.constants" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 &reftitle.constants;
 &extension.constants;
 <variablelist>

  <varlistentry xml:id="msgpack.constants.opt-phponly">
   <term>
    <constant>MessagePack::OPT_PHPONLY</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>To be used with <methodname>MessagePack::setOption</methodname> and
     <methodname>MessagePackUnpacker::setOption</methodname>.</simpara>
     <simpara>See <link linkend="ini.msgpack.php_only">msgpack.php_only</link></simpara>
   </listitem>
  </varlistentry>

  <varlistentry xml:id="msgpack.constants.opt-assoc">
   <term>
    <constant>MessagePack::OPT_ASSOC</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>To be used with <methodname>MessagePack::setOption</methodname> and
     <methodname>MessagePackUnpacker::setOption</methodname>.</simpara>
    <simpara>See <link linkend="ini.msgpack.assoc">msgpack.assoc</link></simpara>
   </listitem>
  </varlistentry>

 </variablelist>
</appendix>

