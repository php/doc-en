<refentry xml:id="function.harudoc-savetostream" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HaruDoc::saveToStream</refname>
  <refpurpose>Save the document into a temporary stream</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>HaruDoc::saveToStream</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Saves the document data into a temporary stream.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns &true; on success.
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   Throws <classname>HaruException</classname> on error. 
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><link linkend="function.harudoc-resetstream"><classname>HaruDoc</classname>::<function>resetStream</function></link></member>
    <member><link linkend="function.harudoc-getstreamsize"><classname>HaruDoc</classname>::<function>getStreamSize</function></link></member>
    <member><link linkend="function.harudoc-readfromstream"><classname>HaruDoc</classname>::<function>readFromStream</function></link></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

