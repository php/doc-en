  <section xml:id="mysqli-stmt.props">
   &reftitle.properties;
   <variablelist>
   <varlistentry xml:id="mysqli-stmt.props.affected-rows">
    <term>
     <varname>affected_rows</varname>
    </term>
    <listitem>
     <para>
      Stores the total number of rows changed, deleted, inserted, or matched
      by the last statement executed.  For further information, see
      <link linkend="mysqli-stmt.affected-rows">mysqli_stmt::$affected_rows</link>.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="mysqli-stmt.props.errno">
    <term>
     <varname>errno</varname>
    </term>
    <listitem>
     <para>
      Stores the error code for the most recent statement call.  For further information, see
      <link linkend="mysqli-stmt.errno">mysqli_stmt::$errno</link>.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="mysqli-stmt.props.error">
    <term>
     <varname>error</varname>
    </term>
    <listitem>
     <para>
      Stores a string description for last statement error.  For further information, see
      <link linkend="mysqli-stmt.error">mysqli_stmt::$error</link>.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="mysqli-stmt.props.error-list">
    <term>
     <varname>error_list</varname>
    </term>
    <listitem>
     <para>
      Stores a list of errors from the last statement executed.  For further information, see
      <link linkend="mysqli-stmt.error-list">mysqli_stmt::$error_list</link>.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="mysqli-stmt.props.field-count">
    <term>
     <varname>field_count</varname>
    </term>
    <listitem>
     <para>
      Stores the number of columns in the given statement.  For further information, see
      <link linkend="mysqli-stmt.field-count">mysqli_stmt::$field_count</link>.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="mysqli-stmt.props.id">
    <term>
     <varname>id</varname>
    </term>
    <listitem>
     <para>
      Stores the statement ID.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="mysqli-stmt.props.insert-id">
    <term>
     <varname>insert_id</varname>
    </term>
    <listitem>
     <para>
      Stores the ID generated from the previous INSERT operation.  For further information, see
      <link linkend="mysqli-stmt.insert-id">mysqli_stmt::$insert_id</link>.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="mysqli-stmt.props.num-rows">
    <term>
     <varname>num_rows</varname>
    </term>
    <listitem>
     <para>
      Stores the number of rows fetched from the server.  For further information, see
      <link linkend="mysqli-stmt.num-rows">mysqli_stmt::$num_rows</link>.
     </para>
    </listitem>
   </varlistentry>
    <varlistentry xml:id="mysqli-stmt.props.param-count">
    <term>
     <varname>param_count</varname>
    </term>
    <listitem>
     <para>
      Stores the number of parameters for the given statement.  For further information, see
      <link linkend="mysqli-stmt.param-count">mysqli_stmt::$param_count</link>.
     </para>
    </listitem>
   </varlistentry>
    <varlistentry xml:id="mysqli-stmt.props.sqlstate">
    <term>
     <varname>sqlstate</varname>
    </term>
    <listitem>
     <para>
      Stores the SQLSTATE error from the previous statement operation.  For further information, see
      <link linkend="mysqli-stmt.sqlstate">mysqli_stmt::$sqlstate</link>.
     </para>
    </listitem>
   </varlistentry>
   </variablelist>
