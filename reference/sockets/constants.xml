 <simpara>
  The following constants are only available on UNIX-like 
  platforms. Each constant is only defined if their equal is 
  available on the platform.
 </simpara>

 <variablelist>
  <varlistentry>
   <term>
    <constant>SOCKET_EPERM</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Operation not permitted.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>SOCKET_ENOENT</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     No such file or directory.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>SOCKET_EINTR</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Interrupted system call.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>SOCKET_EIO</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     I/O error.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>SOCKET_ENXIO</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     No such device or address.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
