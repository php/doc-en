<refentry xml:id="cairo.version" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Cairo::version</refname>
  <refname>cairo_version</refname>
  <refpurpose>Retrieves cairo's library version</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <para>&style.oop; (method):</para>
   <methodsynopsis>
   <modifier>public</modifier> <modifier>static</modifier> <type>int</type><methodname>Cairo::version</methodname>
   <void />
   </methodsynopsis>
  <para>&style.procedural;:</para>
  <methodsynopsis>
   <type>int</type><methodname>cairo_version</methodname>
   <void />
  </methodsynopsis>
  <para>
   Retrieves the current version of the cairo library as an integer value
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Current Cairo library version integer
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>&style.oop;</title>
    <programlisting role="php">
<![CDATA[
<?php
var_dump(Cairo::version());
?>
]]>
     </programlisting>
     &example.outputs.similar;
     <screen>
<![CDATA[
int(10808)
]]>
    </screen>
   </example>
  </para>
  <para>
   <example>
    <title>&style.procedural;</title>
    <programlisting role="php">
<![CDATA[
<?php
var_dump(cairo_version());
?>
]]>
     </programlisting>
     &example.outputs.similar;
     <screen>
<![CDATA[
int(10808)
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>Cairo::versionString</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

