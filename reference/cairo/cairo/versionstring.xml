<refentry xml:id="cairo.versionstring" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Cairo::versionString</refname>
  <refname>cairo_version_string</refname>
  <refpurpose>Retrieves cairo version as string</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <para>Object oriented style (method):</para>
   <methodsynopsis>
   <modifier>public</modifier> <modifier>static</modifier> <type>string</type><methodname>Cairo::versionString</methodname>
   <void />
   </methodsynopsis>
  <para>Procedural style:</para>
  <methodsynopsis>
   <type>string</type><methodname>cairo_version_string</methodname>
   <void />
  </methodsynopsis>
  <para>
   Retrieves the current cairo library version as a string.
  </para>
 
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Current Cairo library version string
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Object oriented style</title>
    <programlisting role="php">
<![CDATA[
<?php

var_dump(Cairo::versionString());

?>
]]>
     </programlisting>
     &example.outputs.similar;
     <screen>
<![CDATA[
string(5) "1.8.8"
]]>
    </screen>
   </example>
  </para>
  <para>
   <example>
    <title>Procedural style</title>
    <programlisting role="php">
<![CDATA[
<?php

var_dump(cairo_version_string());

?>
]]>
     </programlisting>
     &example.outputs.similar;
     <screen>
<![CDATA[
string(5) "1.8.8"
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>Cairo::version</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

