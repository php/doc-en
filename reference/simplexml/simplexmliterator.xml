<reference xml:id="class.simplexmliterator" role="class" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xi="http://www.w3.org/2001/XInclude">
 <title>The SimpleXMLIterator class</title>
 <titleabbrev>SimpleXMLIterator</titleabbrev>

 <partintro>

<!-- {{{ SimpleXMLIterator intro -->
  <section xml:id="simplexmliterator.intro">
   &reftitle.intro;
   <para>
    The SimpleXMLIterator provides recursive iteration over all nodes of a <classname>SimpleXMLElement</classname> object.
   </para>
  </section>
<!-- }}} -->

  <section xml:id="simplexmliterator.synopsis">
   &reftitle.classsynopsis;

<!-- {{{ Synopsis -->
   <classsynopsis class="class">
    <ooclass>
     <classname>SimpleXMLIterator</classname>
    </ooclass>

    <ooclass>
     <modifier>extends</modifier>
     <classname>SimpleXMLElement</classname>
    </ooclass>

    <classsynopsisinfo role="comment">&InheritedMethods;</classsynopsisinfo>
    <xi:include xpointer="xmlns(db=http://docbook.org/ns/docbook) xpointer(id('class.simplexmlelement')/db:refentry/db:refsect1[@role='description']/descendant::db:constructorsynopsis[@role='SimpleXMLElement'])">
     <xi:fallback/>
    </xi:include>
    <xi:include xpointer="xmlns(db=http://docbook.org/ns/docbook) xpointer(id('class.simplexmlelement')/db:refentry/db:refsect1[@role='description']/descendant::db:methodsynopsis[@role='SimpleXMLElement'])">
     <xi:fallback/>
    </xi:include>
   </classsynopsis>

  </section>

<!-- {{{ SimpleXMLIterator properties
  <section xml:id="simplexmliterator.props">
   &reftitle.properties;
   <variablelist>
    <varlistentry xml:id="simplexmliterator.props.name">
     <term><varname>name</varname></term>
     <listitem>
