<reference xml:id="class.parle-token" role="class" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xi="http://www.w3.org/2001/XInclude">

 <title>The Parle\Token class</title>
 <titleabbrev>Parle\Token</titleabbrev>

 <partintro>

<!-- {{{ Parle\Token intro -->
  <section xml:id="parle-token.intro">
   &reftitle.intro;
   <para>
    This class represents a token. Lexer returns instances of this class.
   </para>
  </section>
<!-- }}} -->

  <section xml:id="parle-token.synopsis">
   &reftitle.classsynopsis;

<!-- {{{ Synopsis -->
   <classsynopsis>
    <ooclass><classname>Parle\Token</classname></ooclass>

<!-- {{{ Class synopsis -->
    <classsynopsisinfo>
     <ooclass>
      <classname>Parle\Token</classname>
     </ooclass>
    </classsynopsisinfo>
<!-- }}} -->
    <classsynopsisinfo role="comment">&Constants;</classsynopsisinfo>
    <fieldsynopsis>
     <modifier>const</modifier>
     <type>int</type>
     <varname linkend="parle-token.constants.eoi">Parle\Token::EOI</varname>
     <initializer>0</initializer>
    </fieldsynopsis>
    <fieldsynopsis>
     <modifier>const</modifier>
     <type>int</type>
     <varname linkend="parle-token.constants.unknown">Parle\Token::UNKNOWN</varname>
     <initializer>-1</initializer>
    </fieldsynopsis>
    <fieldsynopsis>
     <modifier>const</modifier>
     <type>int</type>
     <varname linkend="parle-token.constants.skip">Parle\Token::SKIP</varname>
     <initializer>-2</initializer>
    </fieldsynopsis>

    <classsynopsisinfo role="comment">&Properties;</classsynopsisinfo>
    <fieldsynopsis>
     <modifier>public</modifier>
     <type>int</type>
     <varname linkend="parle-token.props.id">id</varname>
    </fieldsynopsis>
    <fieldsynopsis>
     <modifier>public</modifier>
     <type>string</type>
     <varname linkend="parle-token.props.value">value</varname>
    </fieldsynopsis>


    <classsynopsisinfo role="comment">&Methods;</classsynopsisinfo>
    <!-- Uncomment if there are some methods, same for the entities item down there. -->
    <!--<xi:include xpointer="xmlns(db=http://docbook.org/ns/docbook) xpointer(id('class.parle-token')/db:refentry/db:refsect1[@role='description']/descendant::db:methodsynopsis[not(@role='procedural')])" />-->
   </classsynopsis>
<!-- }}} -->

  </section>


<!-- {{{ Parle\Token properties -->
  <section xml:id="parle-token.props">
   &reftitle.properties;
   <variablelist>
    <varlistentry xml:id="parle-token.props.id">
     <term><varname>id</varname></term>
     <listitem>
      <para>Token id.</para>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="parle-token.props.value">
     <term><varname>value</varname></term>
     <listitem>
      <para>Token value.</para>
     </listitem>
    </varlistentry>
   </variablelist>
  </section>
<!-- }}} -->


<!-- {{{ Parle\Token constants -->
  <section xml:id="parle-token.constants">
   &reftitle.constants;
   <variablelist>

    <varlistentry xml:id="parle-token.constants.eoi">
     <term><constant>Parle\Token::EOI</constant></term>
     <listitem>
      <para>End of input token id.</para>
     </listitem>
    </varlistentry>

    <varlistentry xml:id="parle-token.constants.unknown">
     <term><constant>Parle\Token::UNKNOWN</constant></term>
     <listitem>
      <para>Unknown token id.</para>
     </listitem>
    </varlistentry>

    <varlistentry xml:id="parle-token.constants.skip">
     <term><constant>Parle\Token::SKIP</constant></term>
     <listitem>
      <para>Skip token id.</para>
     </listitem>
    </varlistentry>

   </variablelist>
  </section>
<!-- }}} -->


 </partintro>

 <!--&reference.parle.entities.parle-token;-->

</reference>

