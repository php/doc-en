<refentry xml:id="solrquery.removemltqueryfield" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>SolrQuery::removeMltQueryField</refname>
  <refpurpose>Removes one of the moreLikeThis query fields</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier><type>SolrQuery</type><methodname>SolrQuery::removeMltQueryField</methodname>
   <methodparam><type>string</type><parameter>queryField</parameter></methodparam>
  </methodsynopsis>
  <para>
   Removes one of the moreLikeThis query fields.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>queryField</parameter></term>
     <listitem>
      <para>
       The query field
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the current SolrQuery object, if the return value is used.
  </para>
 </refsect1>

</refentry>

