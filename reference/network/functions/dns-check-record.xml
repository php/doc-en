  <refentry id="function.dns-check-record">
   <refnamediv>
    <refname>dns_check_record</refname>
    <refpurpose>Synonym for <function>checkdnsrr</function></refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>int</type><methodname>dns_check_record</methodname>
      <methodparam><type>string</type><parameter>host</parameter></methodparam>
      <methodparam choice="opt"><type>string</type><parameter>type</parameter></methodparam>
     </methodsynopsis>
    <para>
      Check DNS records corresponding to a given Internet host name or
      IP address
    </para>
   </refsect1>
  </refentry>

