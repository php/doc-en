 <refsect1 role="examples">
  &reftitle.examples;
  <para>
    <example>
      <title> <function>Gmagick::setCompressionQuality</function></title>
      <programlisting role="php">
      <![CDATA[
<?php
$gm = new Gmagick();
$gm->read("magick:rose");
$gm->setCompressionQuality(2);
?>
