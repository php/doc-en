<refentry xml:id="function.newt-resume" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>newt_resume</refname>
  <refpurpose>Resume using the newt interface after calling
  <function>newt_suspend</function></refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>newt_resume</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Resume using the newt interface after calling
   <function>newt_suspend</function>.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>newt_suspend</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

