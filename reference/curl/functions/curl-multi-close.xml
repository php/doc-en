  <refentry id="function.curl-multi-close">
   <refnamediv>
    <refname>curl_multi_close</refname>
    <refpurpose>
     Close a set of cURL handles
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>void</type><methodname>curl_multi_close</methodname>
      <methodparam><type>resource</type><parameter>mh</parameter></methodparam>
     </methodsynopsis>
    
    &warn.undocumented.func;
    
    <simpara>
     See also
     <function>curl_multi_init</function> and 
     <function>curl_close</function>.
    </simpara>
   </refsect1>
  </refentry>

