 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>A <function>gc_enabled</function> example</title>
    <programlisting role="php">
<![CDATA[
<?php
if(gc_enabled()) gc_collect_cycles();
?>
]]>
    </programlisting>
   </example>
