<?xml version="1.0" encoding="utf-8"?>

<refentry xml:id="gender-gender.similarnames" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Gender\Gender::similarNames</refname>
  <refpurpose>Get similar names</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>array</type><methodname>Gender\Gender::similarNames</methodname>
   <methodparam><type>string</type><parameter>name</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>country</parameter></methodparam>
  </methodsynopsis>
  <para>
    Get similar names for the given name and country.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
    <varlistentry>
      <term><parameter>name</parameter></term>
        <listitem>
          <para>
            Name to check.
          </para>
       </listitem>
     </varlistentry>
    <varlistentry>
      <term><parameter>country</parameter></term>
        <listitem>
          <para>
            Country id identified by Gender class constant. If ommited ANY_COUNTRY is used.
          </para>
       </listitem>
     </varlistentry>
   </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
    Returns an array with the similar names found.
  </para>
 </refsect1>


</refentry>

