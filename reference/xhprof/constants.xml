<appendix xml:id="xhprof.constants" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 &reftitle.constants;
 &extension.constants;
 <variablelist>
  <varlistentry>
   <term>
    <constant>XHPROF_FLAGS_NO_BUILTINS</constant>
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Used to skip all built-in (internal) functions.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>XHPROF_FLAGS_CPU</constant>
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Used to add <acronym>CPU</acronym> profiling information to the output.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>XHPROF_FLAGS_MEMORY</constant>
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Used to add memory profiling information to the output.
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>
</appendix>

