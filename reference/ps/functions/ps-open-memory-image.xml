<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.ps-open-memory-image">
 <refnamediv>
  <refname>ps_open_memory_image</refname>
  <refpurpose>Takes an GD image and returns an image for placement in a PS document</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>ps_open_memory_image</methodname>
   <methodparam><type>resource</type><parameter>psdoc</parameter></methodparam>
   <methodparam><type>int</type><parameter>gd</parameter></methodparam>
  </methodsynopsis>
  <simpara>
   &warn.undocumented.func;
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>psdoc</parameter></term>
    <listitem>
     <simpara>
      Resource identifier of the postscript file
      as returned by <function>ps_new</function>.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>gd</parameter></term>
    <listitem>
     <simpara>
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

</refentry>
