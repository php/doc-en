<refentry xml:id="function.ps-open-memory-image" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ps_open_memory_image</refname>
  <refpurpose>Takes an GD image and returns an image for placement in a PS document</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>ps_open_memory_image</methodname>
