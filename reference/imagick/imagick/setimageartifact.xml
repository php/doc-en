<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagick-setimageartifact">
 <refnamediv>
  <refname>Imagick::setImageArtifact</refname>
  <refpurpose>Set image artifact</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>Imagick::setImageArtifact</methodname>
   <methodparam><type>string</type><parameter>artifact</parameter></methodparam>
   <methodparam><type>string</type><parameter>value</parameter></methodparam>
  </methodsynopsis>
  <para>
   Associates an artifact with the image. The difference between image properties and 
   image artifacts is that properties are public and artifacts are private.
   &imagick.method.available.0x657;
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>artifact</parameter></term>
     <listitem>
      <para>
       The name of the artifact
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>value</parameter></term>
     <listitem>
      <para>
       The value of the artifact
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &imagick.return.success;
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   &imagick.imagick.throws;
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>Imagick::getImageArtifact</function></member>
    <member><function>Imagick::deleteImageArtifact</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

