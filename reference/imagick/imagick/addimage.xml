<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagick.addimage">
 <refnamediv>
  <refname>Imagick::addImage</refname>
  <refpurpose>Adds new image to Imagick object image list</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>bool</type><methodname>Imagick::addImage</methodname>
   <methodparam><type>Imagick</type><parameter>source</parameter></methodparam>
  </methodsynopsis>
  <para>
   Adds new image to Imagick object from the current position of the source object.
   After the operation iterator position is moved at the end of the list.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>source</parameter></term>
     <listitem>
      <para>
       The source Imagick object
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &imagick.return.success;
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   &imagick.imagick.throws;
  </para>
 </refsect1>

</refentry>

