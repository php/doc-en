<refentry xml:id="imagick.tostring" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Imagick::__toString</refname>
  <refpurpose>Returns the image as a string</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="oop">
   <modifier>public</modifier> <type>string</type><methodname>Imagick::__toString</methodname>
   <void />
  </methodsynopsis>
  <para>
   Returns the current image as string. This will only return a single image; it should not be used for Imagick objects that contain multiple images e.g. an animated GIF or PDF with multiple pages.
  </para>
 </refsect1>
 
 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the string content on success or an empty string on failure.
  </para>
 </refsect1>
 
 
 <refsect1 role="seealso">
  &reftitle.seealso;
  <simplelist>
   <member><methodname>Imagick::getImageBlob</methodname></member>
   <member><methodname>Imagick::getImagesBlob</methodname></member>   
  </simplelist>
 </refsect1>
 
</refentry>

