<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagick.gethomeurl">
 <refnamediv>
  <refname>Imagick::getHomeURL</refname>
  <refpurpose>Returns the ImageMagick home URL</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>Imagick::getHomeURL</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Returns the ImageMagick home URL.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns a link to the imagemagick homepage.
  </para>
 </refsect1>

</refentry>

