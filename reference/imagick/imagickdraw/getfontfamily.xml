<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagickdraw.getfontfamily">
 <refnamediv>
  <refname>ImagickDraw::getFontFamily</refname>
  <refpurpose>Returns the font family</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>string</type><methodname>ImagickDraw::getFontFamily</methodname>
   <void/>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Returns the font family to use when annotating with text.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the font family currently selected or false if font family is not set. 
  </para>
 </refsect1>

</refentry>

