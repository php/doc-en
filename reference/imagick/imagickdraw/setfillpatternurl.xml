<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagickdraw.setfillpatternurl">
 <refnamediv>
  <refname>ImagickDraw::setFillPatternURL</refname>
  <refpurpose>Sets the URL to use as a fill pattern for filling objects</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>bool</type><methodname>ImagickDraw::setFillPatternURL</methodname>
   <methodparam><type>string</type><parameter>fill_url</parameter></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Sets the URL to use as a fill pattern for filling objects. Only local URLs
   ("#identifier") are supported at this time. These local URLs are normally
   created by defining a named fill pattern with DrawPushPattern/DrawPopPattern.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>fill_url</parameter></term>
     <listitem>
      <para>
       URL to use to obtain fill pattern.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

</refentry>

