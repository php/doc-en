<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagickdraw-setstrokewidth">
 <refnamediv>
  <refname>ImagickDraw::setStrokeWidth</refname>
  <refpurpose>Sets the width of the stroke used to draw object outlines</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>ImagickDraw::setStrokeWidth</methodname>
   <methodparam><type>float</type><parameter>stroke_width</parameter></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Sets the width of the stroke used to draw object outlines.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>stroke_width</parameter></term>
     <listitem>
      <para>
       stroke width
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

</refentry>

