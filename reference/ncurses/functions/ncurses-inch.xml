<refentry xml:id="function.ncurses-inch" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ncurses_inch</refname>
  <refpurpose>Get character and attribute at current position </refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>ncurses_inch</methodname>
   <void/>
  </methodsynopsis>
   &warn.experimental.func;
  <para>
   Returns the character from the current position.
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the character, as a string.
  </para>
 </refsect1>
</refentry>

