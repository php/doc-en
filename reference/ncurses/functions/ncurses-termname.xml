<refentry xml:id="function.ncurses-termname" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ncurses_termname</refname>
  <refpurpose>Returns terminals (short)-name</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>ncurses_termname</methodname>
   <void/>
  </methodsynopsis>
  &warn.experimental.func;
  <para>
   Returns terminals shortname.
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the shortname of the terminal, truncated to 14 characters.
   On errors, returns &null;.
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>ncurses_longname</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

