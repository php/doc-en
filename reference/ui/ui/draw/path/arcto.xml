<refentry xml:id="ui-draw-path.arcto" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>UI\Draw\Path::arcTo</refname>
  <refpurpose>Draw an Arc</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <methodname>UI\Draw\Path::arcTo</methodname>
   <methodparam><type>UI\Point</type><parameter>point</parameter></methodparam>
   <methodparam><type>float</type><parameter>radius</parameter></methodparam>
   <methodparam><type>float</type><parameter>angle</parameter></methodparam>
   <methodparam><type>float</type><parameter>sweep</parameter></methodparam>
   <methodparam><type>float</type><parameter>negative</parameter></methodparam>
  </methodsynopsis>
  <para>
   Shall map the path for an arc
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>point</parameter></term>
    <listitem>
     <para>
      The point to begin mapping
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>radius</parameter></term>
    <listitem>
     <para>
      The radius of the arc
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>angle</parameter></term>
    <listitem>
     <para>
      The angle of the arc
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>sweep</parameter></term>
    <listitem>
     <para>
      The sweep of the arc
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>negative</parameter></term>
    <listitem>
     <para>
      
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   
  </para>
 </refsect1>


