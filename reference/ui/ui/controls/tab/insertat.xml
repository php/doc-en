<refentry xml:id="ui-controls-tab.insertat" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>UI\Controls\Tab::insertAt</refname>
  <refpurpose>Insert Page</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <methodname>UI\Controls\Tab::insertAt</methodname>
   <methodparam><type>string</type><parameter>name</parameter></methodparam>
   <methodparam><type>int</type><parameter>page</parameter></methodparam>
   <methodparam><type>UI\Control</type><parameter>control</parameter></methodparam>
  </methodsynopsis>
  <para>
   Shall insert a new page into this Tab
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>name</parameter></term>
    <listitem>
     <para>
      The name for the new page
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>page</parameter></term>
    <listitem>
     <para>
      The index to perform the insertion before
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>control</parameter></term>
    <listitem>
     <para>
      The control for the new page
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

</refentry>

