<?xml version="1.0" encoding="utf-8"?>
<refentry xml:id="uri-rfc3986-uri.equals" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>Uri\Rfc3986\Uri::equals</refname>
  <refpurpose>Compare this URI to another URI for equality</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="Uri\\Rfc3986\\Uri">
   <modifier>public</modifier> <type>bool</type><methodname>Uri\Rfc3986\Uri::equals</methodname>
   <methodparam><type>Uri\Rfc3986\Uri</type><parameter>uri</parameter></methodparam>
   <methodparam choice="opt"><type>Uri\UriComparisonMode</type><parameter>comparisonMode</parameter><initializer>Uri\UriComparisonMode::ExcludeFragment</initializer></methodparam>
  </methodsynopsis>
  <para>
   Compares two URI instances, optionally including or excluding the fragment component.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>uri</parameter></term>
    <listitem><para>The URI to compare with.</para></listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>comparisonMode</parameter></term>
    <listitem><para>Controls whether the fragment is included in the comparison.</para></listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns true if the URIs are considered equal, otherwise false.
  </para>
 </refsect1>
</refentry>
