<?xml version="1.0" encoding="utf-8"?>
<refentry xml:id="uri-whatwg-url.tounicodestring" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>Uri\WhatWg\Url::toUnicodeString</refname>
  <refpurpose>Get the URL as a Unicode string</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="Uri\\WhatWg\\Url">
   <modifier>public</modifier> <type>string</type><methodname>Uri\WhatWg\Url::toUnicodeString</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Returns the URL serialized as a Unicode string.
  </para>
  <note>
   <para>
    This method is similar to <methodname>Uri\WhatWg\Url::toAsciiString</methodname>, but preserves Unicode characters
    in the output.
   </para>
  </note>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the Unicode serialized URL as a <type>string</type>.
  </para>
 </refsect1>
</refentry>
