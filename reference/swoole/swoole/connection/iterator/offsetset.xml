<refentry xml:id="swoole-connection-iterator.offsetset" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Swoole\Connection\Iterator::offsetSet</refname>
  <refpurpose>Assign a Connection to the specified offset.</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>void</type><methodname>Swoole\Connection\Iterator::offsetSet</methodname>
   <methodparam><type>int</type><parameter>offset</parameter></methodparam>
   <methodparam><type>mixed</type><parameter>connection</parameter></methodparam>
  </methodsynopsis>
  <para>
    Assign a Connection to the specified offset.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>offset</parameter></term>
    <listitem>
     <para>
      The offset to assign the Connection to.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>connection</parameter></term>
    <listitem>
     <para>
      The connection to set.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   
  </para>
 </refsect1>


</refentry>

