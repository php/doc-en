<refentry xml:id="swoole-atomic.add" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Swoole\Atomic::add</refname>
  <refpurpose>Add a number to the value to the atomic object.</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>Swoole\Atomic::add</methodname>
   <methodparam choice="opt"><type>int</type><parameter>add_value</parameter></methodparam>
  </methodsynopsis>
  <para>
    Add a number to the value to the atomic object.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>add_value</parameter></term>
    <listitem>
     <para>
      The value which will be added to the current value.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   The new value of the atomic object.
  </para>
 </refsect1>


</refentry>

