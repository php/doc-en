<refentry xml:id="swoole-process.kill" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Swoole\Process::kill</refname>
  <refpurpose>Send signal to the child process.</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <modifier>static</modifier> <type>bool</type><methodname>Swoole\Process::kill</methodname>
   <methodparam><type>int</type><parameter>pid</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>signal_no</parameter></methodparam>
  </methodsynopsis>
  <para>
    Send signal to the child process.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>pid</parameter></term>
    <listitem>
     <para>
      Process pid
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>signal_no</parameter></term>
    <listitem>
     <para>
      Signal to be sent
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>


</refentry>

