<refentry xml:id="oci-collection.max" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>OCI-Collection::max</refname>
  <refpurpose>Returns the maximum number of elements in the collection</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="oop">
   <type>int</type><methodname>OCI-Collection::max</methodname>
   <void />
  </methodsynopsis>
  <para>
   Returns the maximum number of elements in the collection.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the maximum number as an integer, or &false; on errors.
  </para>
  <para>
   If the returned value is 0, then the number of elements is not limited.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><xref linkend="oci-collection.size" /></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

