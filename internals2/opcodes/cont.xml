 <section xml:id="internals2.opcodes.cont.code">
  <title>PHP code</title>
  <programlisting role="php">
   <![CDATA[
<?php
/*
 * Continue next iteration of current loop level
 * opcode number: 51
 */
$foo = false;
while(!$foo) {
    $foo = true;
    continue;
}
?>
